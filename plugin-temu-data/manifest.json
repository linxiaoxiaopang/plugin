{
  "manifest_version": 3,
  "name": "temu数据拦截器",
  "version": "1.0",
  "description": "捕获并展示浏览器请求头信息",
  "permissions": [
    "webRequest",
    "storage",
    "tabs",
    "windows",
    "activeTab",
    "scripting",
    "cookies",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.temu.com/mall.html*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "content/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  }
}

