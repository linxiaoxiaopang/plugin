{
  "manifest_version": 3,
  "name": "月度总计",
  "version": "1.0",
  "description": "月度总计表格处理",
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "windows",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["big.js", "content.js"]
    }
  ]
}
